  "id": "bannerpp",
  "version": "${version}",

  "name": "Banner++",
  "description": "More banner and shield patterns.",
  "authors": [
    "kvverti"
  ],
  "contact": {
    "homepage": "https://github.com/kvverti/banner-plus-plus",
    "sources": "https://github.com/kvverti/banner-plus-plus"
  },

